{
  "name" : "ChatSurfing",
  "version" : "0.1",
  "manifest_version" : 2,
  "description" : "サンプル拡張",
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "notifications"
  ],

  "background" : {
    "page": "background.html"
  },

  "browser_action" : {
    "default_icon" : "./images/logo-white.png",
    "default_title" :  "ChatSurfing",
    "default_popup" : "popup.html"
  }
}
